$(function ($) {
    $('#files').files();
});
$(window).load(function() {
    var tsConfig = {
        //sortForce: [[0, 0]],
        headers: {
            0: { sorter: 'name' },
            1: { sorter: 'abbrdate' },
            2: { sorter: 'filesize' },
            3: { sorter: 'download_count'}
        },
        textExtraction: function (node) {
            return node.innerHTML;
        }
    };

    if ($('#files_list tbody tr').length) {
        $('#files_list').tablesorter(tsConfig).on("sortEnd", SF.tablesorter_svg_update);
        $('#files_list tr th#parent_folder').unbind();
        $('#files_list tr th.header').not('.typesort').not('#parent_folder').addClass('usersortable').append('<svg class="svgico icon-sort"><use xlink:href="#sort"></use></svg>');
    }
});
</script>


        
        








        
